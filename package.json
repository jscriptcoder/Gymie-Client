{
  "name": "gymie",
  "version": "1.0.0",
  "description": "Client and Server that allows to train AI agents written in Javascript on OpenAI Gym environments",
  "main": "src/client/GymieClient.js",
  "scripts": {
    "kill-process": "./src/server/shutdown.sh",
    "start-server": "python src/server/gymie_server.py",
    "test-client_server-down": "ts-node tests/GymieClient_server-down.test.ts",
    "test-server": "python tests/test_gymie_server.py",
    "pretest-client": "yarn start-server &",
    "test-client": "ts-node tests/GymieClient.test.ts",
    "pretest-env": "yarn start-server &",
    "test-env": "ts-node tests/Env.test.ts",
    "test": "yarn test-server && yarn test-client_server-down && yarn test-client && yarn test-env",
    "posttest": "yarn kill-process 5000",
    "random-py-agent": "python tests/run_random_agent.py",
    "prerandom-js-agent": "yarn start-server &",
    "random-js-agent": "ts-node --script-mode --transpile-only tests/runRandomAgent.ts",
    "predocs": "rm -rf docs",
    "docs": "typedoc --out docs src/client",
    "prebuild": "rm -rf dist",
    "build": "tsc src/client/*.ts --outDir dist",
    "release": "yarn docs && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jscriptcoder/Gymie.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "python",
    "reinforcement-learning",
    "openai-gym",
    "tensorflowjs"
  ],
  "author": "Francisco Ramos <francisco.ramos@researchlab.ai>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jscriptcoder/Gymie/issues"
  },
  "homepage": "https://github.com/jscriptcoder/Gymie#readme",
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/tape": "^4.13.0",
    "@types/webgl2": "0.0.5",
    "@types/websocket": "^1.0.1",
    "@types/yargs": "^15.0.8",
    "tape": "^5.0.1",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^2.6.0",
    "chalk": "^4.1.0",
    "websocket": "^1.0.32",
    "yargs": "^16.0.3"
  }
}
